<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Asset_install_date_less_than_1_year__c</fullName>
    <formula>AND ( (TODAY() - Asset.InstallDate &lt;= 365) , NOT( ISNULL(Asset.InstallDate)) , NOT(ISBLANK(Asset.InstallDate) ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Asset install date less than 1 year</label>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Checkbox</type>
</CustomField>
